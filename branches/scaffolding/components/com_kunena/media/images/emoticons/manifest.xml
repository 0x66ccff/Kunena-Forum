<install type="emoticons" version="1.5.0" method="upgrade">
    <name>JXtended - Default</name>
    <version>1.0</version>
    <creationDate>November 2008</creationDate>
    <author>JXtended</author>
    <authorEmail>info@jxtended.com</authorEmail>
    <authorUrl>http://jxtended.com</authorUrl>
    <copyright>2008 JXtended, LLC. All rights reserved.</copyright>
    <license>GNU General Public License</license>
    <description>Default JXtended emoticon set.</description>

	<icons>
		<icon name="Angry Face" file="Angry Face.png" palette="true">
			<code>&gt;:o</code>
			<code>&gt;:-O</code>
		</icon>
		<icon name="Blush" file="Blush.png" palette="true">
			<code>:-[</code>
			<code>:[</code>
		</icon>
		<icon name="Crying" file="Crying.png" palette="true">
			<code>:'(</code>
		</icon>
		<icon name="Foot In Mouth" file="Foot In Mouth.png">
			<code>:!</code>
			<code>:|</code>
			<code>:-!</code>
			<code>:-|</code>
		</icon>
		<icon name="Frown" file="Frown.png" palette="true">
			<code>:-(</code>
			<code>:(</code>
			<code></code>
		</icon>
		<icon name="Gasp" file="Gasp.png">
			<code>:O</code>
			<code>=O</code>
			<code>:o</code>
			<code>=-o</code>
			<code>=-O</code>
			<code>:-O</code>
		</icon>
		<icon name="Grin" file="Grin.png" palette="true">
			<code>:-D</code>
			<code>:D</code>
		</icon>
		<icon name="Halo" file="Halo.png">
			<code>O:-)</code>
			<code>O:)</code>
			<code>o:-)</code>
			<code>o:)</code>
		</icon>
		<icon name="Kiss" file="Kiss.png" palette="true">
			<code>:-*</code>
			<code>:*</code>
		</icon>
		<icon name="Lips are Sealed" file="Lips are Sealed.png">
			<code>:X</code>
			<code>:-x</code>
			<code>:-X</code>
			<code>:x</code>
		</icon>
		<icon name="Money Mouth" file="Money-mouth.png">
			<code>:$</code>
			<code>$)</code>
			<code>:-$</code>
			<code>$-)</code>
		</icon>
		<icon name="Smile" file="Smile.png" palette="true">
			<code>:-)</code>
			<code>:)</code>
		</icon>
		<icon name="Sticking Out Tongue" file="Sticking Out Tongue.png" palette="true">
			<code>:P</code>
			<code>:-p</code>
			<code>:-P</code>
			<code>:p</code>
		</icon>
		<icon name="Undecided" file="Undecided.png" palette="true">
			<code>:\</code>
			<code>:-\</code>
			<code>:-/</code>
		</icon>
		<icon name="Wearing Sunglasses" file="Wearing Sunglasses.png" palette="true">
			<code>8-)</code>
			<code>8)</code>
		</icon>
		<icon name="Wink" file="Wink.png" palette="true">
			<code>;-)</code>
			<code>;)</code>
		</icon>
	</icons>
</install>
